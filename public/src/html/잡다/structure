  i.resizeMarkers = function() {
                h.leafletMap.getZoom() <= 4 ? (n(h.markerSelector).not(".small").addClass("small"),
                n(h.popupSelector).not(".small").addClass("small")) : (n(h.markerSelector + ".small").removeClass("small"),
                n(h.popupSelector + ".small").removeClass("small"))
            }